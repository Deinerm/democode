@article{bessonneau_vivo_2017-1,
	title = {In vivo microsampling to capture the elusive exposome},
	volume = {7},
	issn = {2045-2322},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC5339820/},
	doi = {10.1038/srep44038},
	abstract = {Loss and/or degradation of small molecules during sampling, sample transportation and storage can adversely impact biological interpretation of metabolomics data. In this study, we performed in vivo sampling using solid-phase microextraction (SPME) in combination with non-targeted liquid chromatography and high-resolution tandem mass spectrometry (LC-MS/MS) to capture the fish tissue exposome using molecular networking analysis, and the results were contrasted with molecular differences obtained with ex vivo SPME sampling. Based on 494 MS/MS spectra comparisons, we demonstrated that in vivo SPME sampling provided better extraction and stabilization of highly reactive molecules, such as 1-oleoyl-sn-glycero-3-phosphocholine and 1-palmitoleoyl-glycero-3-phosphocholine, from fish tissue samples. This sampling approach, that minimizes sample handling and preparation, offers the opportunity to perform longitudinal monitoring of the exposome in biological systems and improve the reliability of exposure-measurement in exposome-wide association studies.},
	urldate = {2017-04-19},
	journal = {Scientific Reports},
	author = {Bessonneau, Vincent and Ings, Jennifer and McMaster, Mark and Smith, Richard and Bragg, Leslie and Servos, Mark and Pawliszyn, Janusz},
	month = mar,
