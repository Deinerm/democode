	file = {ACS Full Text Snapshot:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/NNXV6ZGQ/acs.analchem.html:text/html;Tian et al_2016_Web Server for Peak Detection, Baseline Correction, and Alignment in.pdf:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/X7UFB3SV/Tian et al_2016_Web Server for Peak Detection, Baseline Correction, and Alignment in.pdf:application/pdf}
}

