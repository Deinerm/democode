	file = {Dührkop et al_2015_Searching molecular structure databases with tandem mass spectra using CSI.pdf:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/HKPNMFTI/Dührkop et al_2015_Searching molecular structure databases with tandem mass spectra using CSI.pdf:application/pdf;Snapshot:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/TW8R836X/12580.html:text/html}
