	keywords = {Bromophenols, Fragmentation pathway, GC–MS/MS, Human biomonitoring, Human urine, PBDEs},
	pages = {70--74},
	file = {Feng et al_2016_Determination of urinary bromophenols (BrPs) as potential biomarkers for human.pdf:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/2BFX75Z8/Feng et al_2016_Determination of urinary bromophenols (BrPs) as potential biomarkers for human.pdf:application/pdf;ScienceDirect Snapshot:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/HCZH66BU/S1570023216301908.html:text/html}
}

