@article{itoi_serum_2017,
	title = {Serum {Metabolomic} {Profiles} for {Human} {Pancreatic} {Cancer} {Discrimination}},
	volume = {18},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	url = {http://www.mdpi.com/1422-0067/18/4/767},
	doi = {10.3390/ijms18040767},
	abstract = {This study evaluated the clinical use of serum metabolomics to discriminate malignant cancers including pancreatic cancer (PC) from malignant diseases, such as biliary tract cancer (BTC), intraductal papillary mucinous carcinoma (IPMC), and various benign pancreaticobiliary diseases. Capillary electrophoresismass spectrometry was used to analyze charged metabolites. We repeatedly analyzed serum samples (n = 41) of different storage durations to identify metabolites showing high quantitative reproducibility, and subsequently analyzed all samples (n = 140). Overall, 189 metabolites were quantified and 66 metabolites had a 20\% coefficient of variation and, of these, 24 metabolites showed significant differences among control, benign, and malignant groups (p \&lt; 0.05; Steel–Dwass test). Four multiple logistic regression models (MLR) were developed and one MLR model clearly discriminated all disease patients from healthy controls with an area under receiver operating characteristic curve (AUC) of 0.970 (95\% confidential interval (CI), 0.946–0.994, p \&lt; 0.0001). Another model to discriminate PC from BTC and IPMC yielded AUC = 0.831 (95\% CI, 0.650–1.01, p = 0.0020) with higher accuracy compared with tumor markers including carcinoembryonic antigen (CEA), carbohydrate antigen 19-9 (CA19-9), pancreatic cancer-associated antigen (DUPAN2) and s-pancreas-1 antigen (SPAN1). Changes in metabolomic profiles might be used to screen for malignant cancers as well as to differentiate between PC and other malignant diseases.},
	language = {en},
	number = {4},
	urldate = {2017-06-02},
	journal = {International Journal of Molecular Sciences},
	author = {Itoi, Takao and Sugimoto, Masahiro and Umeda, Junko and Sofuni, Atsushi and Tsuchiya, Takayoshi and Tsuji, Shujiro and Tanaka, Reina and Tonozuka, Ryosuke and Honjo, Mitsuyoshi and Moriyasu, Fuminori and Kasuya, Kazuhiko and Nagakawa, Yuichi and Abe, Yuta and Takano, Kimihiro and Kawachi, Shigeyuki and Shimazu, Motohide and Soga, Tomoyoshi and Tomita, Masaru and Sunamura, Makoto},
	month = apr,
	year = {2017},
	note = {00000 
bibtex: itoi2017},
	keywords = {biliary tract cancers, capillary electrophoresis mass spectrometry, Metabolomics, pancreatic cancer},
	pages = {767},
	file = {Itoi et al_2017_Serum Metabolomic Profiles for Human Pancreatic Cancer Discrimination.pdf:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/DEUXGSEX/Itoi et al_2017_Serum Metabolomic Profiles for Human Pancreatic Cancer Discrimination.pdf:application/pdf;Snapshot:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/NHS5M2P7/767.html:text/html}
}