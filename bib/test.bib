	note = {00000 
bibtex: yao2017},
	keywords = {Metabolomics, notoginseng saponins, traditional Chinese medicine, Xuesaitong, Xueshuantong},
	pages = {834--841},
	file = {Snapshot:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/53W6JDAW/abstract.html:text/html;Yao et al_2017_UHPLC–Q-TOF-MS-based metabolomics approach to compare the saponin compositions.pdf:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/5U4TXDFH/Yao et al_2017_UHPLC–Q-TOF-MS-based metabolomics approach to compare the saponin compositions.pdf:application/pdf}
