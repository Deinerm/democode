	note = {00008 
bibtex: fernandez-albert2014},
	pages = {2899--2905},
	file = {Fernández-Albert et al_2014_Intensity drift removal in LC-MS metabolomics by common variance compensation.pdf:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/K6TWJNGK/Fernández-Albert et al_2014_Intensity drift removal in LC-MS metabolomics by common variance compensation.pdf:application/pdf;Fernández-Albert et al_2014_Intensity drift removal in LC-MS metabolomics by common variance compensation.pdf:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/K2ZHX48J/Fernández-Albert et al_2014_Intensity drift removal in LC-MS metabolomics by common variance compensation.pdf:application/pdf;Snapshot:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/KJAEFX35/2899.html:text/html;Snapshot:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/QFGVIHEK/2899.html:text/html;Snapshot:/Users/yufree/Library/Application Support/Zotero/Profiles/dzypl25z.default/zotero/storage/BEH6F6JN/2899.html:text/html}
